{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "pages/api/auth/[...nextauth].js",
    "pages/sign-in",
    "components/DraggableRoadmap.jsx",
    ".next/types/**/*.ts"
, "components/Habits/ParentHabit.jsx", "components/Home/Benefits.jsx", "components/Home/Body.jsx", "components/Layout/ExplorePages.jsx", "components/Home/About.jsx", "components/Home/ReviewCard.jsx", "components/Home/Hero.jsx", "components/ext.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
